{
    "name": "telephony-gateway-service",
    "version": "1.0.0",
    "description": "FairGo Telephony Gateway Service - FreeSWITCH integration and call routing microservice",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "type-check": "tsc --noEmit",
        "migrate": "prisma migrate dev",
        "generate": "prisma generate",
        "studio": "prisma studio"
    },
    "keywords": [
        "telephony",
        "freeswitch",
        "sip",
        "ivr",
        "voip"
    ],
    "author": "FairGo Team",
    "license": "UNLICENSED",
    "dependencies": {
        "@prisma/client": "^5.0.0",
        "axios": "^1.6.0",
        "fastify": "^4.18.0",
        "ioredis": "^5.3.0",
        "joi": "^17.0.0",
        "prisma": "^5.0.0",
        "winston": "^3.8.0",
        "ws": "^8.0.0",
        "@fastify/cors": "^8.0.0",
        "@fastify/helmet": "^10.0.0",
        "@fastify/rate-limit": "^8.0.0",
        "@fastify/compress": "^6.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.0.0",
        "@types/node": "^20.0.0",
        "@types/supertest": "^6.0.0",
        "@types/ws": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "jest": "^29.0.0",
        "supertest": "^6.0.0",
        "ts-jest": "^29.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "prisma": {
        "schema": "prisma/schema.prisma"
    }
}